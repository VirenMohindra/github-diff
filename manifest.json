{
  "manifest_version": 3,
  "name": "GitHub Diff",
  "version": "0.0.1",
  "description": "A simple extension to help with GitHub PRs by copying diff contents to ChatGPT and opening raw files.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon128.jpg",
      "48": "assets/icon128.jpg",
      "128": "assets/icon128.jpg"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "icons": {
    "16": "assets/icon128.jpg",
    "48": "assets/icon128.jpg",
    "128": "assets/icon128.jpg"
  }
}
